{
  "name": "routepath",
  "version": "0.1.0",
  "description": "",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run watch:compile\" \"npm run watch:dev\"",
    "start": "NODE_ENV=production node dist/main.js",
    "build": "swc src -d dist",
    "clean": "rm -rf dist",
    "watch:compile": "swc src -w --out-dir dist",
    "watch:dev": "nodemon --watch \"dist/**/*\" -e js ./dist/main.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "@types/node": "^18.11.18",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
